{
  "name": "wdio-cucumberjs-json-reporter-with-typescript",
  "version": "2.0.2",
  "description": "A WDIO reporter that creates CucumberJS JSON files for WebdriverIO V5+",
  "main": "./build/reporter.js",
  "scripts": {
    "clean": "rimraf coverage build .tmp",
    "compile": "babel lib/ --ignore **/*.spec.js --ignore **/mocks.js -d build/",
    "test.lint": "eslint lib",
    "test": "jest --coverage",
    "release": "np",
    "precompile": "npm run clean",
    "prerelease": "npm run compile",
    "prepublish": "npm run compile",
    "watch": "npm run compile -- --watch",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "update-package-json-dependencies": "npx ncu -u && npm i",
    "eslint-fix-all": "npx eslint lib/**/**/*.ts lib/**/*.ts lib/*.ts --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wswebcreation/wdio-cucumberjs-json-reporter.git"
  },
  "keywords": [
    "cucumberjs",
    "reporter",
    "report",
    "webdriverio",
    "wdio",
    "wdio-reporter"
  ],
  "author": "Wim Selles <wim@wswebcreation.nl>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wswebcreation/wdio-cucumberjs-json-reporter/issues"
  },
  "homepage": "https://github.com/wswebcreation/wdio-cucumberjs-json-reporter#readme",
  "dependencies": {
    "@cucumber/gherkin": "^16.0.0",
    "@wdio/logger": "^6.10.10",
    "@wdio/reporter": "^6.11.0",
    "fs-extra": "^9.1.0"
  },
  "peerDependencies": {
    "@wdio/cucumber-framework": "^6.11.1"
  },
  "devDependencies": {
    "@babel/cli": "7.12.10",
    "@babel/core": "7.12.10",
    "@babel/preset-env": "7.12.11",
    "@babel/register": "7.12.10",
    "@types/fs-extra": "9.0.6",
    "@types/long": "^4.0.1",
    "@types/node": "14.14.22",
    "@typescript-eslint/eslint-plugin": "4.14.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.14.1",
    "@typescript-eslint/parser": "4.14.1",
    "@wdio/cucumber-framework": "^6.11.1",
    "@wdio/sync": "6.11.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-transform-function-bind": "6.22.0",
    "coveralls": "3.1.0",
    "eslint": "7.18.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "31.4.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-tslint": "3.1.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "np": "7.2.0",
    "npm-check-updates": "11.1.1",
    "rimraf": "3.0.2",
    "strip-ansi": "6.0.0",
    "ts-jest": "26.4.4",
    "tslint": "6.1.3",
    "typescript": "4.1.3",
    "webdriverio": "^6.12.1",
    "yargs": "16.2.0"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test.lint && npm run test"
    }
  }
}
